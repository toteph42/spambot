{
	"name" : "toteph42/spambot",
	"type" : "contao-bundle",
	"description" : "Block spam bot access to your Contao web site. ",
	"license" : "GPL-3.0+",
	"homepage" : "https://github.com/toteph42/spambot",
	"authors" : [{
			"name" : "Florian Daeumling",
			"homepage" : "https://github.com/toteph42"
		}
	],
	"keywords" : [
		"spambot",
		"httpbl",
		"blacklist",
		"whitelist",
		"ham",
		"spam",
